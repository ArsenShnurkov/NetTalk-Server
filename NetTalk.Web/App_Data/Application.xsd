<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="config">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="organization" type="xs:string" />
        <xs:element name="serverup" type="xs:boolean" />
        <xs:element name="domain" type="xs:string" />
        <xs:element name="path" type="xs:string" />
        <xs:element name="lastlogclear">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="year" type="xs:unsignedShort" />
              <xs:element name="month" type="xs:unsignedByte" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="access">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="changepic" type="xs:boolean" />
              <xs:element name="changestatus" type="xs:boolean" />
              <xs:element name="changepass" type="xs:boolean" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>